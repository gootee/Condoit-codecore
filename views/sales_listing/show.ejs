<script>
  function load () {

    const strataName1 = "Koret Lofts A+"
    const strataName2 = "Canadian B"

    let favouriteStratas
    favouriteStratas = document.querySelector(".sidebar_favourite_stratas")

    //console.log(favouriteStratas)
    htmlString = "<div class='favourite_strata'>" +
      strataName1 +
      "</div>";  
    favouriteStratas.insertAdjacentHTML('beforeend', htmlString);

    htmlString = "<div class='favourite_strata'>" +
      strataName2 +
      "</div>";  
    favouriteStratas.insertAdjacentHTML('beforeend', htmlString);

    const hearts = document.querySelectorAll(".heart")
    hearts.forEach(heart => {
      heart.addEventListener("click", toggleHeart)
    })
  }

  load();

  function toggleHeart(event) {
    const unitName = event.currentTarget.id
    const favouriteUnits = document.querySelector(".sidebar_favourite_units")
    htmlString = "<div class='favourite_unit'>" +
        unitName +
      "</div>"  
    event.target.classList.toggle("fas");
    event.target.classList.toggle("far");
    favouriteUnits.insertAdjacentHTML('beforeend', htmlString);
  }
</script>

