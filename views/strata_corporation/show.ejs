
<%- include("../partials/header") %>

  <div class="container">
    <div class="strata_show_top app_area">
      <div class="strata_show_cover_image app_area">
        <p>Cover image</p>
      </div>
      <div class="strata_show_basic_info app_area">
        <p>Basic Info</p>
        <div class="strata_show_name app_area">
          <p>Name</p>
          <a href="/stratas/<%= strata_corporation.id %>">              
            <h2>
              <%= strata_corporation.name %> <span> <%= strata_corporation.strata_plan_number %> </span>
            </h2>        
          </a> 
          id: <%= strata_corporation.id%>            
        </div>
        <div class="strata_show_details app_area"> 
          <p>Details</p>
        </div>              
      </div>
      <div class="strata_show_buttons app_area">
        <div class="heart">
            <i class="heart outline icon"></i>
        </div>  
        <form action="/stratas/<%= strata_corporation.id %>" method="POST">
          <input type="hidden" value="DELETE" name="_method">
          <input type="submit" value="Delete"">
        </form>
        <a href="/stratas/<%= strata_corporation.id %>/edit">Edit</a>            
      </div>

    </div>

    <div class="strata_show_info app_area">
