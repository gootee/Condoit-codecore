<div class="main">
<div class="container">
  <div class="strata_index_heading app_area">

    <div class="strata_index_heading_left app_area">
      <div class="strata_index_heading_icons app_area">
        <p>Icons</p>
      </div>      
      <div class="strata_index_heading_message app_area">
        <p>Messages</p>
      </div>
    </div>
    <div class="strata_index_heading_right app_area">
      <div class="strata_index_heading_menu app_area">
        <p>Menu icon</p>
      </div>
    </div>    
  </div>
  
  <div class="strata_listings">
    <ul>
      <% for (let strata_corporation of strata_corporations) { %>
        <li>


          
          <div class="strata_listing app_area">
            <div class="strata_listing_left">
              <div class="strata_listing_cover_image">
                <img src="<%= strata_corporation.image_url%>" alt="Strata photo" height="150" width="150">
              </div>
              <div class="strata_listing_data">
                <div class="strata_listing_name">
                  <a href="/stratas/<%= strata_corporation.id %>">              
                    <div class="strata_listing_name_data">
                      <h2><%= strata_corporation.name%></h2>
                      <h5>     <%= strata_corporation.strata_plan_number %>  (id: <%= strata_corporation.id%>)</h5> 
                                                
                    </div>
                  </a> 
                </div>
                <div class="details_listing"> 
                  <ul>
                    <li>
                      <div class="detail_item">
                        <div class="detail_item_icon">
                          <i class="far fa-building"></i>
                        </div>
                        <div class="detail_item_description">
                          <p>Construction type</p>
                        </div>
                      </div>
                    </li>
                    <li>
                      <div class="detail_item">
                        <div class="detail_item_icon">
                          <i class="fas fa-map-marker-alt"></i>
                        </div>
                        <div class="detail_item_description">
                          <p>Location area</p>
                        </div>
                      </div>
                    </li>
                    <li>
                      <div class="detail_item">
                        <div class="detail_item_icon">
                          <i class="fas fa-user-tie"></i>
                        </div>
                        <div class="detail_item_description">
                          <p>Units for sale</p>
                        </div>
                      </div>
                    </li>
                  </ul>
                  
                </div>              
              </div>              
              
            </div>

            

            <div class="strata_listing_right">
              <div class="scoring">
                <!-- <div class="overall_score circle"> -->
                <div class="overall_score app_area">
                  <p>A+</p>
                </div>
                <div class="score_up_and_down">
                  <div class="score_up_and_down_item score_up  app_area">
                    <div class="thumb_icon">
                      <i class="far fa-thumbs-up fa-2x"></i>
                    </div>
                    <div class="score_items score_up_items">
                      <ul>
                        <li>
                          <div class="score_item">
                            <p>Listing score up item 1</p>
                          </div>                          
                        </li>
                        <li>
                          <div class="score_item">
                            <p>Listing score up item 2</p>
                          </div>                          
                        </li>
                        <li>
                          <div class="score_item">
                            <p>Listing score up item 3</p>
                          </div>                          
                        </li>
                      </ul>
                    </div>
                  </div>  
                  <div class="score_up_and_down_item score_down app_area">
                    <div class="thumb_icon">
                      <i class="far fa-thumbs-down fa-2x"></i>
                    </div>
                    <div class="score_items score_down_items">
                      <ul>
                        <li>
                          <div class="score_item">
                            <p>Listing score up item 1</p>
                          </div>                          
                        </li>
                        <li>
                          <div class="score_item">
                            <p>Listing score up item 2</p>
                          </div>                          
                        </li>
                        <li>
                          <div class="score_item">
                            <p>Listing score up item 3</p>
                          </div>                          
                        </li>
                      </ul>
                    </div>
                  </div> 
                </div>
              </div>
              <div class="heart">
                <i class="far fa-heart fa-3x"></i>
              </div>  
            </div>
          </div>
  
                
                
          <!-- </div> -->
                      
            <!-- <%# if (cluck.image_url && cluck.image_url.length > 0) { %>
              <div class="cluckImage">
                <img src=<%#= cluck.image_url %> alt="Cluck image">
              </div>                      -->
            <!-- <%# } %>                -->
          <!-- </div> -->
          <!-- </div> -->
        </li>
    
      <% } %>
    </ul>
  </div>

      
