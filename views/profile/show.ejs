<%- include("../partials/header") %>
<div class="container profile_container">
  <div class="profile_left app_area">
    <div class="profile_us app_area">
      <div class="profile_us_heading">
        <h1>Us</h1>
        <div class="buttons">
          <div class="edit_button">
            <i data-feather="edit-3"></i>
          </div>
        </div>          
      </div>

      <div class="profile_us_top">
        <div class="us_icons">
          <div class="occupant_icons">
            <div class="occupant">
              <i data-feather="user" class="us_icon"></i>
              <div class="icon_name">
                <p>Jackie</p>
              </div>
            </div>  

            <div class="occupant">
              <i data-feather="user" class="us_icon"></i>
              <div class="icon_name">
                <p>John</p>
              </div>
            </div>                        
          </div>

          <div class="pet_icons">
            <div class="pet">
              <i class="fas fa-dog fa-3x us_icon"></i>
              <div class="icon_name">
                <p>Olive</p>                
              </div>
            </div>                       
          </div>  

          <div class="vehicle_icons">
            <div class="vehicle">
              <i class="fas fa-plus fa-2x us_icon"></i>
            </div>  
            <div class="vehicle">
              <i class="fas fa-bicycle fa-3x us_icon"></i>
              <div class="icon_name">
                <p>Bicycle</p>                
              </div>
            </div>     
            <div class="vehicle">
              <i class="fas fa-car fa-3x us_icon"></i>
              <div class="icon_name">
                <p>Car</p>                
              </div>
            </div>                   
          </div>        
      </div>
    
    </div>

 
    <div class="details app_area">
      <div class="list_of_items occupants">
        <div class="list_heading">
          <h4>Occupants</h4>
        </div>
        <div>
          <ul>
            <li>
              <div class="list_item">
                <div class="list_item_buttons buttons">
                  <div class="delete_button">
                    <i data-feather="x"></i>
                  </div>
                  <div class="edit_button">
                    <i data-feather="edit-3"></i>
                  </div>                
                </div>
                <div class="list_item_cover_image">
                  <!-- <p>Cover image</p> -->
                  <i data-feather="user" class="list_item_icon"></i>
                </div>
                <div class="list_item_name">
                  <p>Jackie Gootee</p>             
                </div>
              </div>             
            </li>
            <li>
              <div class="list_item">
                <div class="list_item_buttons buttons">
                  <div class="delete_button">
                    <i data-feather="x"></i>
                  </div>
                  <div class="edit_button">
                    <i data-feather="edit-3"></i>
                  </div>                
                </div>
                <div class="list_item_cover_image">
                  <!-- <p>Cover image</p> -->
                  <i data-feather="user" class="list_item_icon"></i>
                </div>
                <div class="list_item_name">
                  <p>John Gootee</p>             
                </div>
              </div>             
            </li>
          </ul>           
        </div>
      </div>
      <div class="list_of_items pets">
        <div class="list_heading">
          <h4>Pets</h4>
        </div>
        <div>        
          <ul>
            <li>
              <div class="list_item">
                <div class="list_item_buttons buttons">
                  <div class="delete_button">
                    <i data-feather="x"></i>
                  </div>
                  <div class="edit_button">
                    <i data-feather="edit-3"></i>
                  </div>   
                </div>
                <div class="list_item_cover_image">
                  <img src="/images/olive.jpg" alt="Pet image" height="25" width="25"> 
                </div>
                <div class="list_item_name">
                  <p>Olive</p>             
                </div>
              </div>             
            </li>
          </ul>    
        </div>        
      </div>
      <div class="list_of_items vehicles">
        <div class="list_heading">
          <h4>Vehicles</h4>
        </div>
        <div>    
          <ul>
            <li>
              <div class="list_item">
                <div class="list_item_buttons buttons">
                  <div class="delete_button">
                    <i data-feather="x"></i>
                  </div>
                  <div class="edit_button">
                    <i data-feather="edit-3"></i>
                  </div>   
                </div>
                <div class="list_item_cover_image">
                  <i class="fas fa-car fa-3x us_icon"></i>
                </div> 
                <div class="list_item_type app_area">
                  <p>Type</p>             
                </div> 
                <div class="list_item_description app_area">
                  <p>Description</p>             
                </div>               
              </div>             
            </li>
            
            <li>
              <div class="list_item">
                <div class="list_item_buttons buttons">
                  <div class="delete_button">
                    <i data-feather="x"></i>
                  </div>
                  <div class="edit_button">
                    <i data-feather="edit-3"></i>
                  </div>   
                </div>
                <div class="list_item_cover_image">
                    <i class="fas fa-bicycle fa-3x us_icon"></i>
                </div> 
                <div class="list_item_type app_area">
                  <p>Type</p>             
                </div> 
                <div class="list_item_description app_area">
                  <p>Description</p>             
                </div>               
              </div>             
            </li>
          </ul>  
        </div>          
      </div>
    </div>
    <div class="bottom app_area">
      <div class="buttons app_area">
        <p>Save</p>
      </div>
    </div>
  </div>

    <div class="profile_financial app_area">
      <div class="profile_financial_heading">
        <h1>Financial</h1>
        <div class="buttons">
          <div class="edit_button">
            <i data-feather="edit-3"></i>
          </div>
        </div>          
      </div>
      <div class="profile_financial_top top app_area">
        <div class="icon app_area">
          <p>icon</p>
        </div>
      </div>

      <div class="details app_area">
        <p>Details</p>
        <div class="budget app_area">

          <ul>
            <li>
              <h4>Budget</h4>              
              <div class="list_item app_area">
                <!-- <div class="list_item_buttons">
                  <div class="edit app_area">
                    <p>E</p>
                  </div>
                </div> -->
                <div class="list_item_icon app_area">
                  <p>icon</p>
                </div>             
                <div class="list_item_type app_area">
                  <p>Monthly Housing Budget</p>             
                </div> 
                <div class="list_item_amount app_area">
                  <p>Amount</p>             
                </div> 
                <div class="list_item_description app_area">
                  <p>Description</p>             
                </div>               
              </div>             
            </li>
            <li>
              <h4>Down Payment</h4>
              <div class="list_item app_area">
                <!-- <div class="list_item_buttons">
                  <div class="edit app_area">
                    <p>E</p>
                  </div>
                </div> -->
                <div class="list_item_icon app_area">
                  <p>icon</p>
                </div>             
                <div class="list_item_type app_area">
                  <p>Down Payment</p>             
                </div> 
                <div class="list_item_amount app_area">
                  <p>Amount</p>             
                </div> 
                <div class="list_item_description app_area">
                  <p>Description</p>             
                </div>               
              </div>             
            </li> 
            <li>
              <p>Savings</p>
              <div class="list_item app_area">
                <!-- <div class="list_item_buttons">
                  <div class="edit app_area">
                    <p>E</p>
                  </div>
                </div> -->
                <div class="list_item_icon app_area">
                  <p>icon</p>
                </div>             
                <div class="list_item_type app_area">
                  <p>Savingst</p>             
                </div> 
                <div class="list_item_amount app_area">
                  <p>Amount</p>             
                </div> 
                <div class="list_item_description app_area">
                  <h4>Description</h4>             
                </div>               
              </div> 

            </li>   
            <li>
              <h4>Transaction</h4>
            </li>         
            
          </ul>            
        </div>

        <div class="lender app_area">
          <h4>Lender:</h4>
          <div class="lender">
            <div class="ui checkbox">
              <input type="checkbox" class="lender_preapproved" name="lender_preapproved" value="lender_preapproved"> 
              <label for="lender_preapproved">I've been pre-approved for my mortgage: </label>
            </div>
            <div class="control_group">  
              <input class="lender_info" style = "display: none" type="text" name="lender_name" placeholder="Lender Name">
              <input class="lender_info" style = "display: none" type="text" name="lender_email" placeholder="Email">
            </div>
          </div>            
        </div>
      </div>
    </div>

    <div class="bottom app_area">
      <div class="buttons app_area">
        <p>Save</p>
      </div>
    </div>
  </div>


  <div class="profile_elements app_area">
    <p>Elements</p>
    <div class="profile_element_plus app_area">
      <i data-feather="plus-circle" class="plus_button"></i>
    </div>
    <div class="profile_element_input_new app_area">
      <p>New input</p>
      <div class="input_fields app_area">
        <div class="icon app_area">
          <div>icon</div>
        </div>
        <p>Inputs</p>
        <div class="importance">
          <select name="importance">
            <option value="required">Required</option>
            <option value="important">Important</option>
            <option value="preferred" selected>Preferred</option>
            <option value="avoid">Avoid</option>
          </select> 
        </div>
      </div>      
      <div class="input_buttons app_area">
        <div class="cancel_button app_area">
          <p>Cancel</p>
        </div>   
        <div class="save_button app_area">
          <p>Save</p>
        </div>             
      </div>


    </div>
    <div class="profile_element_importance_levels app_area">
      <p>Importance levels</p>
      <div class="profile_element_importance_level required app_area">
        <p>Required</p>
        <ul>
          <li>
            <div class="profile_element app_area">
              <div class="buttons">
                <div class="delete_button">
                  <i data-feather="x"></i>
                </div>                
                <div class="edit_button">
                  <i data-feather="edit-3"></i>
                </div>
              </div>               
              <div class="profile_element_icon app_area">
                <p>icon</p>
              </div>
              <div class="profile_element_name app_area">
                <p>name</p>
              </div>
              <div class="profile_element_importance app_area">
                <p>importance</p>
              </div>              
            </div>
          </li>
        </ul>
      </div>
      <div class="profile_element_importance_level important app_area">
        <p>Important</p>
      </div>
      <div class="profile_element_importance_level preferred app_area">
        <p>Preferred</p>
      </div>
      <div class="profile_element_importance_level avoid app_area">
        <p>Avoid</p>
      </div>
    </div>
  </div>
</div>

